        with net_manager send_all_bpart_stats()
    }
} else
{
    net_manager.party_leader = false
    net_manager.in_party = false
    net_manager.bot_match = false
    leave_matchmaking()
    sendMessageReal("syncPlayersConnected",FL_NORMAL)
}
