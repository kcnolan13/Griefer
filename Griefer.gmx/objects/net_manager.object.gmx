else if instance_exists(local_player) and not objVarRead(local_player,"alive") and instance_exists(rag_target)
{
    draw_killed_by = true
    do_zoom = true
    view_lerp = 0.25
    view_overscaler = lerp(view_overscaler,view_overscaler_target,0.05)
    view_xtarget = lerp(view_xview+view_wview/2,rag_target.x,view_lerp)
    view_ytarget = lerp(view_yview+view_hview/2,rag_target.y,view_lerp)
}

//view zooming
if do_zoom
{
    view_hwidth = 1365*view_overscaler*VIEW_SCALE
    
    if view_hwidth &gt; room_width
        view_hwidth = room_width
        
    view_vwidth = (9/16)*view_hwidth
    
    if view_vwidth &gt; room_height
    {
        view_vwidth = room_height
        view_hwidth = 16/9*view_vwidth
    }
    
    view_wview = ceil(lerp(view_wview,view_hwidth,view_lerp))
    view_hview = ceil(lerp(view_hview,view_vwidth,view_lerp))
    
    if instance_exists(modal_controls) and in_match()
    {
        view_wview = 1365
        view_hview = 9/16*1365
    }
    
    view_xview = view_xtarget - view_wview/2//lerp(view_xview, view_xtarget - view_wview/2,view_lerp)
    view_yview = view_ytarget - view_hview/2//lerp(view_xview, view_ytarget - view_hview/2,view_lerp)
    
    keep_view_in_bounds()
}

    

