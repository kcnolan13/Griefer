            //printf("targeting local_player: "+objVarRead(local_player,"pName")+": @ ("+view_xtarget+", "+view_ytarget+")")
        }
        else
        {
            view_xtarget = lerp(view_xview+view_wview/2,room_width/2,view_lerp)
            view_ytarget = lerp(view_yview+view_hview/2,room_height/2,view_lerp)
        }
        
        if room_counter &lt; 10.5*room_speed and instance_exists(local_player) and objVarRead(local_player,"visible")
        {
            view_overscaler = 0.55
        }
        else
        {
            if match_ending()
                view_overscaler = 1.15
            else
                view_overscaler = 1
        }
    }
}
//local player is bot
else if (instance_exists(local_player) and is_bot(local_player))
{
    do_zoom = true
    if bot_focus_counter
        bot_focus_counter --
        
    view_lerp = 0.06
    
    if bot_focus_counter and instance_exists(bot_focus)
    {
        view_overscaler = 0.85
        view_xtarget = lerp(view_xview+view_wview/2,bot_focus.x,view_lerp)
        view_ytarget = lerp(view_yview+view_hview/2,bot_focus.y,view_lerp)
        if not objVarRead(bot_focus,"alive")
        {
            bot_focus_counter = 0
        }
    }
    else
    {
        view_overscaler = 2.5
        view_xtarget = lerp(view_xview+view_wview/2,room_width/2,view_lerp)
        view_ytarget = lerp(view_yview+view_hview/2,room_height/2,view_lerp)
        if random(1) &lt; 1/(4*room_speed)
        {
            var target_ind = floor(random_range(0,instance_number(player)+0.99))
            bot_focus = instance_find(player,target_ind)
            
            if not instance_exists(bot_focus)
                bot_focus = noone
            else
            {
                printf("ERROR (not): focusing on new bot")
                bot_focus_counter = random_range(4*room_speed,6*room_speed)
            }
        }
    }
}
